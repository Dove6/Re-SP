#
# LAMPA
#
OBJECT=LAMPA
LAMPA:TYPE=ANIMO
LAMPA:VISIBLE=TRUE
LAMPA:FILENAME=LAMPA.ANN
LAMPA:TOCANVAS=TRUE
LAMPA:PRIORITY=0
LAMPA:FPS=16
LAMPA:PRELOAD=TRUE
LAMPA:RELEASE=TRUE
LAMPA:MONITORCOLLISION=FALSE
LAMPA:MONITORCOLLISIONALPHA=FALSE
LAMPA:ONINIT={THIS^PLAY("PLAY");}
LAMPA:ONSTARTED={NOWAGRABUT^ENABLE();}
LAMPA:ONFINISHED={THIS^PLAY("PLAY");}


# BEHINIT_JEZYK
#
OBJECT=BEHINIT_JEZYK
BEHINIT_JEZYK:TYPE=BEHAVIOUR
BEHINIT_JEZYK:CODE={GR_WYBOR_J^ADD("BUT_POLAK","BUT_CZECH");WYBOR_BKG^SHOW();GR_WYBOR_J^ENABLE();NOWAGRABUT^DISABLEBUTVISIBLE();BUT_LAK^SETPRIORITY(400);BUT_LAK^ENABLE();LAMPA^PAUSE();}
#
# B_SET_CZECH
#
OBJECT=B_SET_CZECH
B_SET_CZECH:TYPE=BEHAVIOUR
B_SET_CZECH:CODE={PIRACI^SETLANGUAGE("0405");GR_WYBOR_J^DISABLE();WYBOR_BKG^HIDE();NOWAGRABUT^ENABLE();BUT_LAK^DISABLE();LAMPA^RESUME();}
#
# B_SET_POLSKI
#
OBJECT=B_SET_POLSKI
B_SET_POLSKI:TYPE=BEHAVIOUR
B_SET_POLSKI:CODE={PIRACI^SETLANGUAGE("040E");GR_WYBOR_J^DISABLE();WYBOR_BKG^HIDE();NOWAGRABUT^ENABLE();BUT_LAK^DISABLE();LAMPA^RESUME();}

