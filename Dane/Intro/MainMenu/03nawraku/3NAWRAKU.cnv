######################################
##############  ANIMO  ###############
######################################

OBJECT=REKIN
REKIN:TYPE=ANIMO
REKIN:FILENAME=REKINY.ANN
REKIN:TOCANVAS=TRUE
REKIN:VISIBLE=TRUE
REKIN:ONSTARTED={REKIN_BUTTON^DISABLEBUTVISIBLE();}
REKIN:ONSTARTED^PLYWA={REKIN_BUTTON^ENABLE();}
REKIN:ONFINISHED^PLYWA={@IF("REKIN^GETPOSITIONX()>550","{REKIN^PLAY("PLYWA");}","{REKIN^PLAY("ZANURZASIE");}");}
REKIN:ONFINISHED^ZANURZASIE={THIS^SETPOSITION(220,0);THIS^PLAY("WYNURZASIE");}
REKIN:ONFINISHED^ZANURZASIE2={THIS^HIDE();REKIN2^PLAY("WYPLYWA");}
REKIN:ONFINISHED^WYNURZASIE={THIS^PLAY("PLYWA");}
REKIN:ONFRAMECHANGED={THIS^MOVE(-2,0);REKIN_BUTTON^SETRECT("REKIN");}

OBJECT=REKIN2
REKIN2:TYPE=ANIMO
REKIN2:FILENAME=REKINY2.ANN
REKIN2:TOCANVAS=TRUE
REKIN2:VISIBLE=FALSE
REKIN2:ONFINISHED^WYPLYWA={THIS^HIDE();REKIN^PLAY("WYNURZASIE");}

OBJECT=FALE1
FALE1:TYPE=ANIMO
FALE1:FILENAME=FALA01.ANN
FALE1:TOCANVAS=TRUE
FALE1:VISIBLE=TRUE
FALE1:ONFINISHED={THIS^PLAY("PLAY");}

OBJECT=FALE2
FALE2:TYPE=ANIMO
FALE2:FILENAME=FALA02.ANN
FALE2:TOCANVAS=TRUE
FALE2:VISIBLE=TRUE
FALE2:ONFINISHED={THIS^PLAY("PLAY");}

OBJECT=FALE3
FALE3:TYPE=ANIMO
FALE3:FILENAME=FALA03.ANN
FALE3:TOCANVAS=TRUE
FALE3:VISIBLE=TRUE
FALE3:ONFINISHED={THIS^PLAY("PLAY");}

OBJECT=REKSIO
REKSIO:TYPE=ANIMO
REKSIO:FILENAME=REKSIO.ANN
REKSIO:TOCANVAS=TRUE
REKSIO:VISIBLE=TRUE
REKSIO:ONFINISHED^WALCZY={THIS^PLAY("WALCZY");}
REKSIO:ONFINISHED^WSIADA={REKSIO^PLAY("LECI");}
REKSIO:ONFINISHED^LECI={G_GRPEP2INI^RESETINI();G_GRPEP1INI^RESETINI();RATUNEK^GOTO("4PLAZA");}

OBJECT=PTAK2
PTAK2:TYPE=ANIMO
PTAK2:FILENAME=PTAK2.ANN
PTAK2:TOCANVAS=TRUE
PTAK2:VISIBLE=TRUE
PTAK2:ONFINISHED^PODLATUJE1={THIS^PLAY("PODLATUJE2");}
PTAK2:ONFINISHED^PODLATUJE2={PTAK2_BUTTON^SETRECT("PTAK2");PTAK2_BUTTON^ENABLE();}


