BEH_CHOWA:CODE={REKIN^HIDE();REKIN_BUTTON^DISABLE();}

OBJECT=BEH_REKINWYN
BEH_REKINWYN:TYPE=BEHAVIOUR
BEH_REKINWYN:CODE={REKIN2^HIDE();REKIN^PLAY("WYNURZASIE");@IF("REXNAZIEMI'1","BEH_REXROZKL","");}

OBJECT=BEH_CHOWA2
BEH_CHOWA2:TYPE=BEHAVIOUR
BEH_CHOWA2:CODE={REKIN^HIDE();REKIN2^HIDE();REKIN_BUTTON^DISABLE();}

OBJECT=BEH_SUMA
BEH_SUMA:TYPE=BEHAVIOUR
BEH_SUMA:CODE={REKIN^SETPOSITION(200,0);REKIN^PLAY("WYNURZASIE");SUMA^SET(0);}


OBJECT=BEH_ONTIMER
BEH_ONTIMER:TYPE=BEHAVIOUR
BEH_ONTIMER:CODE={REKIN^MOVE(-3,0);SUMA^INC();}

OBJECT=TIMER
