EXITBUTTON:DRAGGABLE=FALSE
EXITBUTTON:GFXSTANDARD=BEXIT
EXITBUTTON:GFXONCLICK=BEXITOC
EXITBUTTON:GFXONMOVE=BEXITOM
EXITBUTTON:ONACTION={RATUNEK^GOTO("MENUGLOWNE");}

OBJECT=PLAZA
PLAZA:TYPE=BUTTON
PLAZA:VISIBLE=TRUE
PLAZA:RECT=560,42,800,212
PLAZA:ENABLE=TRUE
PLAZA:DRAGGABLE=FALSE
PLAZA:ONACTION={3PLAZA^PLAY();}

OBJECT=PTAK2_BUTTON
PTAK2_BUTTON:TYPE=BUTTON
PTAK2_BUTTON:RECT=PTAK2
PTAK2_BUTTON:ONACTION={THIS^DISABLEBUTVISIBLE();PTAK2^PLAY("PODLATUJE1");}
PTAK2_BUTTON:ENABLE=FALSE

OBJECT=REKSIO_BUTTON
REKSIO_BUTTON:TYPE=BUTTON
REKSIO_BUTTON:GFXSTANDARD=REKSIO
REKSIO_BUTTON:ONACTION={THIS^DISABLEBUTVISIBLE();REKSIO^PLAY("WSIADA");3SURFING1^PLAY();}
REKSIO_BUTTON:ENABLE=TRUE

OBJECT=REKIN_BUTTON
REKIN_BUTTON:TYPE=BUTTON
REKIN_BUTTON:RECT=REKIN
REKIN_BUTTON:ONACTION={REKIN^PLAY("ZANURZASIE2");}
REKIN_BUTTON:ENABLE=TRUE


