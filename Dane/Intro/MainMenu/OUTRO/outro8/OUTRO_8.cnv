######################################
##############  ANIMO  ###############
######################################

OBJECT=KRET
KRET:TYPE=ANIMO
KRET:VISIBLE=TRUE
KRET:FILENAME=KRET.ANN
KRET:TOCANVAS=TRUE
KRET:PRIORITY=0
KRET:FPS=16
KRET:PRELOAD=TRUE
KRET:RELEASE=TRUE
KRET:MONITORCOLLISION=FALSE
KRET:MONITORCOLLISIONALPHA=FALSE
KRET:ONFINISHED^1={THIS^PLAY("1");}
KRET:ONFINISHED^2={UFOKURA^PLAY("1");}
KRET:ONFINISHED^3={THIS^PLAY("3");}
KRET:ONFINISHED^4={THIS^PLAY("5");UFOKURA^PLAY("2");KOSMOKURA4^PLAY();}
KRET:ONFINISHED^5={THIS^PLAY("5");}

OBJECT=UFOKURA
UFOKURA:TYPE=ANIMO
UFOKURA:VISIBLE=FALSE
UFOKURA:FILENAME=UFOKURA.ANN
UFOKURA:TOCANVAS=TRUE
UFOKURA:PRIORITY=0
UFOKURA:FPS=16
UFOKURA:PRELOAD=TRUE
UFOKURA:RELEASE=TRUE
UFOKURA:MONITORCOLLISION=FALSE
UFOKURA:MONITORCOLLISIONALPHA=FALSE
UFOKURA:ONFINISHED^1={KRET^PLAY("3");KRET10^PLAY();}
UFOKURA:ONFINISHED^2={THIS^PLAY("2");}
UFOKURA:ONFINISHED^3={END^PLAY("PLAY");}

OBJECT=END
END:TYPE=ANIMO
END:VISIBLE=FALSE
END:FILENAME=END.ANN
END:TOCANVAS=TRUE
END:PRIORITY=100
END:FPS=16
END:PRELOAD=TRUE
END:RELEASE=TRUE
END:MONITORCOLLISION=FALSE
END:MONITORCOLLISIONALPHA=FALSE
END:ONFINISHED={RATUNEK^GOTO("OFERTA");}

OBJECT=START
START:TYPE=ANIMO
START:VISIBLE=TRUE
START:FILENAME=START.ANN
START:TOCANVAS=TRUE
START:PRIORITY=100
START:FPS=16
START:PRELOAD=TRUE
START:RELEASE=TRUE
START:MONITORCOLLISION=FALSE
START:MONITORCOLLISIONALPHA=FALSE
START:ONFINISHED={THIS^HIDE();}


######################################
##############  IMAGE  ###############
######################################

OBJECT=KLAPA
KLAPA:TYPE=IMAGE
KLAPA:VISIBLE=TRUE
KLAPA:FILENAME=KLAPA.IMG
KLAPA:TOCANVAS=TRUE
KLAPA:PRIORITY=60
KLAPA:PRELOAD=TRUE
KLAPA:RELEASE=TRUE
KLAPA:MONITORCOLLISION=FALSE
KLAPA:MONITORCOLLISIONALPHA=FALSE


######################################
##############  SOUND  ###############
######################################

OBJECT=KRET1
KRET1:TYPE=SOUND
KRET1:FILENAME=KRET1.WAV
KRET1:PRELOAD=FALSE
KRET1:RELEASE=FALSE
KRET1:ONFINISHED={KRET^PLAY("2");KLAPA^HIDE();}

OBJECT=KRET10
KRET10:TYPE=SOUND
KRET10:FILENAME=KRET10.WAV
KRET10:PRELOAD=FALSE
KRET10:RELEASE=FALSE
KRET10:ONFINISHED={KRET^PLAY("4");}

OBJECT=KOSMOKURA4
KOSMOKURA4:TYPE=SOUND
KOSMOKURA4:FILENAME=KOSMOKURA4.WAV
KOSMOKURA4:PRELOAD=FALSE
KOSMOKURA4:RELEASE=FALSE
KOSMOKURA4:ONFINISHED={UFOKURA^PLAY("3");}


######################################
############  BEHAVIOUR  #############
######################################

OBJECT=__INIT__
__INIT__:TYPE=BEHAVIOUR
__INIT__:CODE={BEHINITSCENEMUSIC^RUN(); !ANIMATIONS_INIT; KRET^PLAY("1");START^PLAY("PLAY"); !SOUNDS_INIT; KRET1^PLAY();}

