######################################
##############  ANIMO  ###############
######################################

OBJECT=KRET
KRET:TYPE=ANIMO
KRET:VISIBLE=TRUE
KRET:FILENAME=KRET.ANN
KRET:TOCANVAS=TRUE
KRET:PRIORITY=0
KRET:FPS=16
KRET:PRELOAD=TRUE
KRET:RELEASE=TRUE
KRET:MONITORCOLLISION=FALSE
KRET:MONITORCOLLISIONALPHA=FALSE
KRET:ONFINISHED={THIS^PLAY("PLAY");}


######################################
##############  SOUND  ###############
######################################

OBJECT=KRET1
KRET1:TYPE=SOUND
KRET1:FILENAME=KRET1.WAV
KRET1:PRELOAD=FALSE
KRET1:RELEASE=FALSE
KRET1:ONFINISHED={THIS^PLAY("PLAY");}

OBJECT=OUTRO7
OUTRO7:TYPE=SOUND
OUTRO7:FILENAME=OUTRO7.WAV
OUTRO7:PRELOAD=FALSE
OUTRO7:RELEASE=FALSE
OUTRO7:ONFINISHED={RATUNEK^GOTO("OUTRO_6");}


######################################
############  BEHAVIOUR  #############
######################################

OBJECT=__INIT__
__INIT__:TYPE=BEHAVIOUR
__INIT__:CODE={BEHINITSCENEMUSIC^RUN(); !ANIMATIONS_INIT; KRET^PLAY("PLAY"); !SOUNDS_INIT; KRET1^PLAY("PLAY");OUTRO7^PLAY("PLAY");}

