######################################
##############  ANIMO  ###############
######################################

OBJECT=REKSIO
REKSIO:TYPE=ANIMO
REKSIO:VISIBLE=TRUE
REKSIO:FILENAME=REKSIO.ANN
REKSIO:TOCANVAS=TRUE
REKSIO:PRIORITY=0
REKSIO:FPS=16
REKSIO:PRELOAD=TRUE
REKSIO:RELEASE=TRUE
REKSIO:MONITORCOLLISION=FALSE
REKSIO:MONITORCOLLISIONALPHA=FALSE
REKSIO:ONFINISHED={THIS^PLAY("PLAY");}

OBJECT=WODA
WODA:TYPE=ANIMO
WODA:VISIBLE=TRUE
WODA:FILENAME=WODA.ANN
WODA:TOCANVAS=TRUE
WODA:PRIORITY=0
WODA:FPS=16
WODA:PRELOAD=TRUE
WODA:RELEASE=TRUE
WODA:MONITORCOLLISION=FALSE
WODA:MONITORCOLLISIONALPHA=FALSE
WODA:ONFINISHED={THIS^PLAY("PLAY");}


######################################
##############  SOUND  ###############
######################################

OBJECT=OUTRO6
OUTRO6:TYPE=SOUND
OUTRO6:FILENAME=OUTRO6.WAV
OUTRO6:PRELOAD=FALSE
OUTRO6:RELEASE=FALSE
OUTRO6:ONFINISHED={RATUNEK^GOTO("OUTRO_5");}


######################################
############  BEHAVIOUR  #############
######################################

OBJECT=__INIT__
__INIT__:TYPE=BEHAVIOUR
__INIT__:CODE={BEHINITSCENEMUSIC^RUN(); !ANIMATIONS_INIT; REKSIO^PLAY("PLAY");WODA^PLAY("PLAY"); !SOUNDS_INIT; OUTRO6^PLAY();}

