#
#TYTUL2
#
OBJECT=TYTUL2
TYTUL2:TYPE=ANIMO
TYTUL2:VISIBLE=TRUE
TYTUL2:FILENAME=TYTUL2.ANN
TYTUL2:TOCANVAS=TRUE
TYTUL2:PRIORITY=100
TYTUL2:FPS=8
TYTUL2:PRELOAD=TRUE
TYTUL2:RELEASE=TRUE
TYTUL2:MONITORCOLLISION=FALSE
TYTUL2:MONITORCOLLISIONALPHA=FALSE
TYTUL2:ONINIT=HIDE
TYTUL2:ONFINISHED=SK2
TYTUL2:ONSTARTED={NOWE3^PLAY();}

#
#REKSIO
#
OBJECT=REKSIO
