######################################
#############BEHAVIOUR#############
######################################

#
#GOTO2
#
OBJECT=GOTO2
GOTO2:TYPE=BEHAVIOUR
GOTO2:CODE={RATUNEK^GOTO("INTRO2");}

#
#PLAY
#
OBJECT=PLAY
PLAY:TYPE=BEHAVIOUR
PLAY:CODE={THIS^PLAY("PLAY");}

#
#ENABLETIMERA
#
OBJECT=ENABLETIMERA
ENABLETIMERA:TYPE=BEHAVIOUR
ENABLETIMERA:CODE={INTROTIMER1^ENABLE();}

#
#3NAPIS
#
OBJECT=3NAPIS
3NAPIS:TYPE=BEHAVIOUR
3NAPIS:CODE={NAPIS1^HIDE();NAPIS2^HIDE();NAPIS3^PLAY("PLAY");INTROTIMER1^DISABLE();}

#
#HIDE
#
OBJECT=HIDE
HIDE:TYPE=BEHAVIOUR
HIDE:CODE={THIS^HIDE();}

#
#THISDISABLE
#
OBJECT=THISDISABLE
THISDISABLE:TYPE=BEHAVIOUR
THISDISABLE:CODE={THIS^DISABLE();}

#
#ENABLETIMERA2
#
OBJECT=ENABLETIMERA2
ENABLETIMERA2:TYPE=BEHAVIOUR
ENABLETIMERA2:CODE={INTROTIMER2^ENABLE();NAPIS1^HIDE();}

#
#4NAPIS
#
OBJECT=4NAPIS
4NAPIS:TYPE=BEHAVIOUR
4NAPIS:CODE={NAPIS3^HIDE();TYTUL^PLAY("PLAY");INTROTIMER2^DISABLE();}

#
#REKSIO1
#
OBJECT=REKSIO1
REKSIO1:TYPE=BEHAVIOUR
REKSIO1:CODE={REKSIO^PLAY("1");NAPIS3^HIDE();}

#
#REKSIO2
#
OBJECT=REKSIO2
REKSIO2:TYPE=BEHAVIOUR
REKSIO2:CODE={INTROTIMER2^DISABLE();REKSIO^PLAY("2");TYTUL1^PLAY("PLAY");}

#
#SK1
#
OBJECT=SK1
SK1:TYPE=BEHAVIOUR
SK1:CODE={SKRZYNIA^PLAY("1");}

#
#SK2
#
OBJECT=SK2
SK2:TYPE=BEHAVIOUR
SK2:CODE={SKRZYNIA^PLAY("2");}

#
#MOVENAPISOW
#
OBJECT=MOVENAPISOW
MOVENAPISOW:TYPE=BEHAVIOUR
MOVENAPISOW:CODE={TYTUL1^MOVE(0,-5);TYTUL2^MOVE(0,5);SKRZYNIA^MOVE(0,5);}

#
#ENABTIM3
#
OBJECT=ENABTIM3
ENABTIM3:TYPE=BEHAVIOUR
ENABTIM3:CODE={INTRO1S^PLAY();INTROTIMER3^ENABLE();}

#
#DOMENUCZYDOWRAKU
#
OBJECT=DOMENUCZYDOWRAKU
DOMENUCZYDOWRAKU:TYPE=BEHAVIOUR
DOMENUCZYDOWRAKU:CODE={@IF("G_BFIRSTENTRY'TRUE","{RATUNEK^GOTO("NAWRAKU");}","{RATUNEK^GOTO("MENUGLOWNE");}");}

OBJECT=__INIT__
__INIT__:TYPE=BEHAVIOUR
__INIT__:CODE={BEHINITSCENEMUSIC^RUN();}

