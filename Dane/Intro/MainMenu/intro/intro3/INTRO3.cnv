REKSIO5:CODE={WRAK^PLAY("PLAY");INTROTIMER5^DISABLE();NUMERBLYSKU^SET(2);BLYSKANIE^RUN();INTRO7B^PLAY();}

#
#DOMENUCZYDOWRAKU
#
OBJECT=DOMENUCZYDOWRAKU
DOMENUCZYDOWRAKU:TYPE=BEHAVIOUR
DOMENUCZYDOWRAKU:CODE={PIERWSZEWEJSCIEDOGRY^SET(1);@IF("PIERWSZEWEJSCIEDOGRY'0","{RATUNEK^GOTO("NAWRAKU");}","{RATUNEK^GOTO("MENUGLOWNE");}");}

