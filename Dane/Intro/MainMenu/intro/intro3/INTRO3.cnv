#############BEHAVIOUR#############
######################################

#
#PLAY
#
OBJECT=PLAY
PLAY:TYPE=BEHAVIOUR
PLAY:CODE={THIS^PLAY("PLAY");}

#
#EXIT
#
OBJECT=EXIT
EXIT:TYPE=BEHAVIOUR
EXIT:CODE={APPLICATION^EXIT();}

#
#THISHIDE
#
OBJECT=THISHIDE
THISHIDE:TYPE=BEHAVIOUR
THISHIDE:CODE={THIS^HIDE();}

#
#BLYSKANIE
#
OBJECT=BLYSKANIE
BLYSKANIE:TYPE=BEHAVIOUR
BLYSKANIE:CODE={INTRO3^RUN(NAZWABLYSKU,"PLAY","PLAY");}

#
#REKSIO1
#
OBJECT=REKSIO1
REKSIO1:TYPE=BEHAVIOUR
REKSIO1:CODE={REKSIO^PLAY("1");INTROTIMER1^DISABLE();BLYSKANIE^RUN();PIXELSEQ^PLAY("PIXELSEQ_7");}

#
#BLYSKI2
#
OBJECT=BLYSKI2
BLYSKI2:TYPE=BEHAVIOUR
BLYSKI2:CODE={NUMERBLYSKU^SET(3);BLYSKANIE^RUN();INTROTIMER2^ENABLE();}

#
#REKSIO2
#
OBJECT=REKSIO2
REKSIO2:TYPE=BEHAVIOUR
REKSIO2:CODE={REKSIO^PLAY("2");INTROTIMER2^DISABLE();NUMERBLYSKU^SET(1);BLYSKANIE^RUN();}

#
#BLYSKI3
#
